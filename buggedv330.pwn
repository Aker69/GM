CMD:heal(playerid, params[])
{
	    new id, string[256], cash, giveplayer[25], sendername[256];
	    GetPlayerName(playerid, sendername, sizeof(sendername));
     	if(IsPlayerInRangeOfPoint(playerid, 500.0, groupVariables[playerVariables[playerid][pGroup]][gGroupInteriorPos][0], groupVariables[playerVariables[playerid][pGroup]][gGroupInteriorPos][1], groupVariables[playerVariables[playerid][pGroup]][gGroupInteriorPos][2]))
		{
		    if(playerVariables[playerid][pDs] == 0)
		    {
				SetPlayerHealth(playerid, 99);
    			format(string, sizeof(string), "* %s ate some food!", sendername);
				nearByMessage(playerid, COLOR_PURPLE, string);
				playerVariables[playerid][pDs] = 60;
			}
			else return SCM(playerid,COLOR_WHITE,"Mai poti folosi aceasta comanda peste 1 minut.");
		}
		else if(InHouse[playerid] >= 1)
		{
			if(playerVariables[playerid][pDs] == 0)
		    {
				SetPlayerHealth(playerid, 99);
    			format(string, sizeof(string), "* %s ate some food!", sendername);
				nearByMessage(playerid, COLOR_PURPLE, string);
				playerVariables[playerid][pDs] = 60;
			}
			else return SCM(playerid,COLOR_WHITE,"Mai poti folosi aceasta comanda peste 1 minut.");
		}
		else
		{
			if(sscanf(params, "ui", id,cash)) return SendClientMessage(playerid, COLOR_WHITE, "USAGE: /heal [playerid] [price]");
			{

			    if(IsPlayerConnected(playerid))
				{
					if(cash < 1 || cash > 5000) { SendClientMessage(playerid, COLOR_GREY, "Healing price not below 1 or above 5000!"); return 1; }
					if (IsPlayerConnected(id))
					{
					    if(id != INVALID_PLAYER_ID)
					    {
							GetPlayerName(id, giveplayer, sizeof(giveplayer));
							new giveambu = GetPlayerVehicleID(id);
							new playambu = GetPlayerVehicleID(playerid);
							if(playerVariables[playerid][pGroup] == 4)
							{
								if((vehicleVariables[playambu][vVehicleGroup] == 4) && playambu == giveambu)
								{
									new Float:tempheal;
									GetPlayerHealth(id,tempheal);
									if(tempheal >= 99.0)
									{
										SendClientMessage(playerid, COLOR_WHITE,"That person is fully healed.");
										return 1;
									}
									format(string, sizeof(string), "~y~Healed~n~~g~+$%d",cash);
									GameTextForPlayer(playerid, string, 5000, 1);
									playerVariables[playerid][pMoney] += cash;
									playerVariables[id][pMoney] -= cash;
								    SetPlayerHealth(id, 99);
									PlayerPlaySound(playerid, 1150, 0.0, 0.0, 0.0);
									PlayerPlaySound(id, 1150, 0.0, 0.0, 0.0);
									format(string, sizeof(string), "~y~Healed ~n~~g~-$%d",cash);
									GameTextForPlayer(id, string, 5000, 1);
								}
								else
								{
									SendClientMessage(playerid, COLOR_WHITE, "One of you is not in the Ambulance!");
									return 1;
								}
							}
						}
					}
				}
			}
		}
		return 1;
}
