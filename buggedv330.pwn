CMD:attack(playerid, params[])
{
	if(playerVariables[playerid][pGroup] == 6 || playerVariables[playerid][pGroup] == 7 || playerVariables[playerid][pGroup] == 8 || playerVariables[playerid][pGroup] == 10 || playerVariables[playerid][pGroup] == 9 || playerVariables[playerid][pGroup] == 11)
	{
	    new samp[256];
	    new deelay = groupVariables[playerVariables[playerid][pGroup]][gDeelay];
		format(samp, sizeof(samp), "Mafia ta mai poate ataca dupa %s minute.", CalculeazaTimp(deelay));
	    if(groupVariables[playerVariables[playerid][pGroup]][gDeelay] <= 0)
	    {
		    if(playerVariables[playerid][pGroupRank] >= 5)
		    {
				new hour,minute,second;
				gettime(hour,minute,second);
				if(((hour >= 20) && (hour <= 21)) || (CanWar == 1))
				{
					new sendername[MAX_PLAYER_NAME];
					new turf, string[256];
					for(new i = 0; i <= sizeof(TurfInfo); i++)
					{
						if(IsPlayerInTurf(playerid, i) == 1)
						{
							turf = i;
							break;
						}
					}
					if(WarInfo[turf][wAttacker] != 0) return SCM(playerid, COLOR_WHITE, "This turf is already attacked!");
					if(turf == 0) return SCM(playerid, COLOR_WHITE, "You aren't in a turf!");
					if(TurfInfo[turf][zOwned] == playerVariables[playerid][pGroup]) return SCM(playerid, COLOR_WHITE, "You can't attack your turf");
					if(groupVariables[TurfInfo[turf][zOwned]][gInWar] == 1) return SCM(playerid, COLOR_WHITE, "This faction is already in a war!");
					if(groupVariables[playerVariables[playerid][pGroup]][gInWar] == 1) return SCM(playerid, COLOR_WHITE, "Your faction is already in a war!");
					GetPlayerName(playerid, sendername, sizeof(sendername));

					WarInfo[turf][wAttacker] = playerVariables[playerid][pGroup];
					WarInfo[turf][wFaction] = TurfInfo[turf][zOwned];
					WarInfo[turf][wTime] = 600;

					format(string, sizeof(string),"[TURF] %s from your group attacked turf %d (owned by %s).",sendername, turf, groupVariables[TurfInfo[turf][zOwned]][gGroupName]);
					SendToGroup(playerVariables[playerid][pGroup], COLOR_ATTACK, string);

					format(string, sizeof(string),"[TURF] %s from %s attacked turf %i owned by you.",sendername, groupVariables[playerVariables[playerid][pGroup]][gGroupName],turf);
					SendToGroup(TurfInfo[turf][zOwned], COLOR_ATTACK, string);

					groupVariables[WarInfo[turf][wFaction]][gScore] = 0;
					groupVariables[WarInfo[turf][wAttacker]][gScore] = 1;

					groupVariables[WarInfo[turf][wFaction]][gInWar] = 1;
					groupVariables[WarInfo[turf][wAttacker]][gInWar] = 1;

					foreach(Player, i)
					{
						if(playerVariables[i][pGroup] == WarInfo[turf][wAttacker] || playerVariables[i][pGroup] == WarInfo[turf][wFaction])
						{
							new time = WarInfo[turf][wTime];

							format(string, sizeof(string), "War time: ~y~%s ~w~", CalculeazaTimp(time));
							TextDrawSetString(tTimer[i], string);
							TextDrawShowForPlayer(i, tTimer[i]);

							playerVariables[i][pWarScore] = 0;
							playerVariables[i][pWarDeaths] = 0;

							GangZoneFlashForPlayer(i, Turfs[turf], COLOR_RED);

						}
					}
				}
				else return SCM(playerid, COLOR_TEAL,"Nu este ora 20 - 22.");
			}
			else return SCM(playerid, COLOR_TEAL,"Nu ai rankul necesar!");
		}
		else return SCM(playerid, COLOR_TEAL, samp);
	}
	else return SCM(playerid, COLOR_TEAL,"Nu esti intr-o mafie!");
	return 1;
}
