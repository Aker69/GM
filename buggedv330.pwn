public antiCheat()
{
	new ammo;
    new weaponid;
	foreach(Player, i)
	{
	    if(playerVariables[i][pStatus] >= 1)
		{
		    if(playerVariables[i][pAdminLevel] < 1)
			{
				if(GetPlayerSpecialAction(i) == SPECIAL_ACTION_USEJETPACK && playerVariables[i][pJetpack] == 0)
				{
					format(szMessage, sizeof(szMessage), "Warning: %s (%d) may be cheating (weapon cheats - jetpack). Keep an eye on him!", playerVariables[i][pNormalName], i);
					submitToHelpersAndAdmins(szMessage, COLOR_RED);
				}
				for (new n=0; n<12; n++)
				{
					GetPlayerWeaponData(i, n, weaponid, ammo);
					if(weaponid == 38 && ammo > 0)
					{
						if(playerVariables[i][pJetpack] == 0)
						{
							format(szMessage, sizeof(szMessage), "Warning: %s (%d) may be cheating (weapon cheats - minigun). Keep an eye on him!", playerVariables[i][pNormalName], i);
							submitToHelpersAndAdmins(szMessage, COLOR_RED);
						}
					}
				}
				if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
				{
					new speed=GetPlayerSpeed(i,0);
					if(speed>300)
					{
						format(szMessage, sizeof(szMessage), "Warning: %s (%d) may be cheating (speed hack in a vehicle). Keep an eye on him!", playerVariables[i][pNormalName], i);
						submitToHelpersAndAdmins(szMessage, COLOR_RED);
					}
				}
				else if(!IsPlayerInAnyVehicle(i))
				{
					new speed=GetPlayerSpeed(i,0);
					if(speed>300)
					{
						format(szMessage, sizeof(szMessage), "Warning: %s (%d) may be cheating (speed hack). Keep an eye on him!", playerVariables[i][pNormalName], i);
						submitToHelpersAndAdmins(szMessage, COLOR_RED);
					}
				}
		    }
		}
	}
	return 1;
}
