CMD:report(playerid, params[])
{
	new string2[256],message[128],admins=0;
	if(sscanf(params, "s[128]", message)) return SCM(playerid, COLOR_GREY, SYNTAX_MESSAGE"/report [text]");
	{
		if(playerVariables[playerid][pRMuted] <= 0)
		{
			if(playerVariables[playerid][pDr] <= 0)
			{
				foreach(Player, i)
		     	{
					if(playerVariables[i][pAdminLevel] >= 1)
					{
						admins++;
					}
				}
				if(admins == 0) return SendClientMessage(playerid, COLOR_WHITE, "Error: There are no admins online.");
				format(string2, sizeof(string2), "Report trimis.");
				SendClientMessage(playerid, COLOR_YELLOW, string2);
				format(string2, sizeof(string2), "Report form %s [%d]: %s.",playerVariables[playerid][pNormalName],playerid, message);
				submitToHelpersAndAdmins(string2,COLOR_RED);
				playerVariables[playerid][pDr] = 60;
				playerVariables[playerid][pReported] = 1;
				format(playerVariables[playerid][pReportMessage], 256,"%s", string2);
			}
			else
			{
				SCM(playerid,-1,"Poti da un report o data la un minut.");
			}
		}
		else SCM(playerid,-1,"You are muted from /report.");
	}
	return 1;
}
