CMD:su(playerid, params[])
{
	new
		id,
		crime[90],
		wLevel;
	if(sscanf(params, "uis[96]", id, wLevel, crime)) return SCM(playerid, -1, "/suspect [playerid] [level] [reason]");
	if(id != INVALID_PLAYER_ID)
	{
		if(playerVariables[playerid][pGroup] == 1 || playerVariables[playerid][pGroup] == 2 || playerVariables[playerid][pGroup] == 3 || playerVariables[playerid][pGroup] == 5)
		{
			if(id != playerid)
			{
				if(playerVariables[id][pGroup] == 1 || playerVariables[id][pGroup] == 2 || playerVariables[id][pGroup] == 3 || playerVariables[id][pGroup] == 5)
				{
					SCM(playerid, -1, "You can't suspect a cop!");
				}
				else
				{
					if(playerVariables[id][pWarrants] + wLevel > 6)
					{
						playerVariables[id][pWarrants] = 6;
						SetPlayerWantedLevel(id, 6);
					}
					else
					{
						playerVariables[id][pWarrants] += wLevel;
						SetPlayerWantedLevel(id, playerVariables[id][pWarrants]);
					}
					new szTargetName[MAX_PLAYER_NAME];
					GetPlayerName(id, szTargetName, MAX_PLAYER_NAME);
					GetPlayerName(playerid, szPlayerName, MAX_PLAYER_NAME);
					format(szMessage, sizeof(szMessage), "Dispatch: %s (%d) has comitted a crime: '%s'. Reporter: %s (%d). W: +%d | New Wanted Level: %d", szTargetName, id, crime, szPlayerName, playerid, wLevel, playerVariables[id][pWarrants]);
					sendDepartmentMessage(COLOR_BLUE, szMessage);
					format(playerVariables[id][pCrimeReason], 90, "%s%s\n", playerVariables[id][pCrimeReason], crime);
					format(szMessage, sizeof(szMessage), "You have commited a crime: %s. W: +%d. New wanted level: %d. Reporter: %s",crime, wLevel,playerVariables[id][pWarrants], playerVariables[playerid][pNormalName]);
					SCM(id, COLOR_RED, szMessage);
				}
			}
			else
			{
				SCM(playerid, -1, "You can't suspect yourself!");
			}
		}
		else
		{
			SCM(playerid, -1, "You are not a cop!");
		}
	}
	return 1;
}

