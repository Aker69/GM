CMD:changeclanrank(playerid, params[])
{
	if(playerVariables[playerid][pClan] >= 1 && playerVariables[playerid][pClanRank] >= 6) {
	    new
			rank,
	        userID;

        if(sscanf(params, "ud", userID, rank)) {
            SendClientMessage(playerid, COLOR_GREY, SYNTAX_MESSAGE"/changeclanrank [playerid] [rank]");
        }
        else
		{
            if(!IsPlayerConnected(userID)) return SendClientMessage(playerid, COLOR_GREY, "The specified userID/name is not connected.");
			else if(rank < 1 || rank > 6) return SendClientMessage(playerid, COLOR_GREY, "Invalid rank specified.");
			else if(playerVariables[playerid][pClan] != playerVariables[userID][pClan]) return SendClientMessage(playerid, COLOR_GREY, "That player isn't in your clan.");
			else if(playerVariables[playerid][pClanRank] <= playerVariables[userID][pClanRank]) return SendClientMessage(playerid, COLOR_GREY, "You can't alter this person's rank.");
			else if(playerVariables[userID][pClanRank] == rank) return SendClientMessage(playerid, COLOR_GREY, "That person is already of that rank.");
			else {

				new
					messageString[128];

				GetPlayerName(playerid, szPlayerName, MAX_PLAYER_NAME);
				format(messageString, sizeof(messageString), "%s has promoted you to the rank %d.", szPlayerName,rank);
				SendClientMessage(userID, COLOR_NICESKY, messageString);

				GetPlayerName(userID, szPlayerName, MAX_PLAYER_NAME);
				format(messageString, sizeof(messageString), "You promoted %s to the rank %d.", szPlayerName, rank);
				SendClientMessage(playerid, COLOR_WHITE, messageString);
				playerVariables[userID][pClanRank] = rank;
			}
        }
	}
	return 1;
}

