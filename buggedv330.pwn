	foreach(Player, x)
	{
	    if(playerVariables[x][pCarID1] == vehicleid && playerVariables[x][pCarLock] == 1)
		{
			SetVehicleParamsForPlayer(vehicleid, forplayerid, 0, 1); Lock[vehicleid] = 1;
	    }
	    if(playerVariables[x][pCarID2] == vehicleid && playerVariables[x][pCarLock2] == 1)
		{
			SetVehicleParamsForPlayer(vehicleid, forplayerid, 0, 1); Lock[vehicleid] = 1;
	    }
