CMD:cuninvite(playerid, params[])
{
	if(playerVariables[playerid][pClan] >= 1 && playerVariables[playerid][pClanRank] >= 6)
	{
	    new userID;
        if(sscanf(params, "u", userID)) return SendClientMessage(playerid, COLOR_GREY, SYNTAX_MESSAGE"/cuninvite [playerid]");
		{
			if(playerVariables[playerid][pClan] == playerVariables[userID][pClan])
			{
				new messageString[119];
	            new name[MAX_PLAYER_NAME];
				GetPlayerName(playerid, name, MAX_PLAYER_NAME);
				format(messageString, sizeof(messageString), "%s has removed you from the %s.", name, clanVariables[playerVariables[userID][pClan]][cClanName]);
				SendClientMessage(userID, COLOR_NICESKY, messageString);

				GetPlayerName(userID, szPlayerName, MAX_PLAYER_NAME);
				format(messageString, sizeof(messageString), "You have removed %s from your clan.", szPlayerName);
				SendClientMessage(playerid, COLOR_WHITE, messageString);

				format(messageString, sizeof(messageString), "%s has left the clan (uninvited).", szPlayerName);
				SendToClan(playerVariables[playerid][pClan], COLOR_GENANNOUNCE, messageString);

				playerVariables[userID][pClan] = 0;
				playerVariables[userID][pClanRank] = 0;
				SetPlayerName(userID,playerVariables[userID][pNormalName]);
			}
        }
	}
	return 1;
}
CMD:lockhq(playerid, params[]) {
	if(playerVariables[playerid][pGroup] >= 1 && playerVariables[playerid][pGroupRank] >= 6) {
		switch(groupVariables[playerVariables[playerid][pGroup]][gGroupHQLockStatus]) {
			case 1: {
			    SendClientMessage(playerid, COLOR_WHITE, "HQ unlocked.");
				groupVariables[playerVariables[playerid][pGroup]][gGroupHQLockStatus] = 0;
				format(szMessage, sizeof(szMessage), "%s's HQ\n\nPress ~k~~F to enter.", groupVariables[playerVariables[playerid][pGroup]][gGroupName]);
			}
			case 0: {
			    SendClientMessage(playerid, COLOR_WHITE, "HQ locked.");
				groupVariables[playerVariables[playerid][pGroup]][gGroupHQLockStatus] = 1;
			    format(szMessage, sizeof(szMessage), "%s's HQ\n\n(locked)", groupVariables[playerVariables[playerid][pGroup]][gGroupName]);
			}
		}

		UpdateDynamic3DTextLabelText(groupVariables[playerVariables[playerid][pGroup]][gGroupLabelID], COLOR_Riffa, szMessage);
	}
	return 1;
}
