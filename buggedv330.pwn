stock SendToGroup(groupid, colour, string[]) {
	if(groupid > 0) {
		foreach(Player, i) {
			if(playerVariables[i][pGroup] == groupid)
			{
				if(playerVariables[i][pStatus] >= 1)
				{
					SendClientMessage(i, colour, string);
				}
			}
		}
	}
	return 1;
}
stock SendToClan(groupid, colour, string[])
{
	if(groupid > 0)
	{
		foreach(Player, i)
		{
			if(playerVariables[i][pStatus] >= 1)
			{
				if(playerVariables[i][pClan] == groupid)
				{
					SendClientMessage(i , colour, string);
				}
			}
		}
	}
	return 1;
}

stock FetchLevelFromHours(const iHours) {
	switch(iHours) {
	    case 0..24: return 1;
	    case 25..48: return 2;
	    case 49..72: return 3;
	    case 73..100: return 4;
	    case 101..175: return 5;
	    case 176..200: return 6;
	    case 201..208: return 8;
	    case 209..336: return 9;
	    case 337..480: return 10;
	}
	return 0;
}

stock SendToFrequency(const frequency, const colour, const string[]) {
	if(frequency > 0) {
		foreach(Player, i) {
			if(playerVariables[i][pWalkieTalkie] == frequency) {
				SendClientMessage(i, colour, string);
			}
		}
	}
	return 1;
}

stock sendDepartmentMessage(const colour, const string[]) {
	foreach(Player, i) {
	    if(groupVariables[playerVariables[i][pGroup]][gGroupType] == 1 || groupVariables[playerVariables[i][pGroup]][gGroupType] == 2) {
	        SendClientMessage(i, colour, string);
		}
	}
	return 1;
}

stock IsKeyJustDown(key, newkeys, oldkeys) {
	if((newkeys & key) && !(oldkeys & key))
		return 1;

	return 0;
}
