		}
	}



	SetCameraBehindPlayer(playerid);

	ResetPlayerWeapons(playerid);

	givePlayerWeapons(playerid);

	SetPlayerWeather(playerid, 1);

	syncPlayerTime(playerid);
	SetPlayerSpawn(playerid);
	TogglePlayerControllable(playerid, true);

	return 1;
