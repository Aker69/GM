stock GetClosestPlayer(const playerid) {
    new
		Float:Distance,
		target = -1;

    foreach(Player, i) {
        if (playerid != i && playerVariables[i][pSpectating] == INVALID_PLAYER_ID && (target < 0 || Distance > GetDistanceBetweenPlayers(playerid, i))) {
            target = i;
            Distance = GetDistanceBetweenPlayers(playerid, i);
        }
    }
    return target;
}

