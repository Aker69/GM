public beeper(veh)
{
	if(alarmed[veh] == 1)
	{
        GetVehiclePos(veh,vehX,vehY,vehZ);
        driverid = GetVehicleDriver(veh);
		for(new x=0; x<BEEP_VOLUME; x++)
		{
		    for(new i=0; i<MAX_PLAYERS; i++)
		    {
		    	if(i != driverid) PlayerPlaySound(i, 1147, vehX,vehY,vehZ);
		    	if(i == driverid) PlayerPlaySound(i, 1147, 0.0,0.0,0.0);
			}
		}
		beeps[veh]++;
		SetTimerEx("beeper",BEEP_INTERVAL*1000, false, "i", veh);
	}
	return 1;
}
public OnPlayerModelSelection(playerid, response, listid, modelid)
{
    if(listid == skinlist)
    {
        if(response)
        {
            SetPlayerSkin(playerid, modelid);
            playerVariables[playerid][pSkin] = modelid;
            SpawnPlayer(playerid);
            SCM(playerid, COLOR_ATTACK,"Skin selected!");
        }
        return 1;
    }
    if(listid == VehList)
    {
        if(response)
        {
        	new
				Float: carSpawnPos[4],
				messageString[64];

			if(systemVariables[vehicleCounts][0] + systemVariables[vehicleCounts][1] + systemVariables[vehicleCounts][2] < MAX_VEHICLES)
			{
				GetPlayerPos(playerid, carSpawnPos[0], carSpawnPos[1], carSpawnPos[2]);
				GetPlayerFacingAngle(playerid, carSpawnPos[3]);

				AdminSpawnedVehicles[vehCount] = CreateVehicle(modelid, carSpawnPos[0], carSpawnPos[1], carSpawnPos[2], carSpawnPos[3], -1, -1, -1);
				systemVariables[vehicleCounts][2]++;

				LinkVehicleToInterior(AdminSpawnedVehicles[vehCount], GetPlayerInterior(playerid));
				SetVehicleVirtualWorld(AdminSpawnedVehicles[vehCount], GetPlayerVirtualWorld(playerid));

				PutPlayerInVehicle(playerid, AdminSpawnedVehicles[vehCount], 0);

				format(messageString, sizeof(messageString), "You have spawned a %s (vehicle ID %d).", VehicleNames[modelid - 400], AdminSpawnedVehicles[vehCount]);
				SendClientMessage(playerid, COLOR_WHITE, messageString);

				vehCount++;
			}
        }
        return 1;
    }
    return 1;
}
