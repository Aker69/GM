				playerName[2][MAX_PLAYER_NAME],
				querySz[150];

			format(querySz, sizeof(querySz), "SELECT `playerName` FROM `playeraccounts` WHERE `playeraccounts`.`playerName` = '%s'", newName);
			new Cache: resultoo = mysql_query (handle, querySz);

			if(cache_get_row_count() > 0) {
			    SendClientMessage(playerid, COLOR_GREY, "That name is already taken.");
			    return 1;
			}
			cache_delete(resultoo);
			GetPlayerName(playerid, playerName[0], MAX_PLAYER_NAME);
			GetPlayerName(iTarget, playerName[1], MAX_PLAYER_NAME);

			mysql_format(handle, querySz, sizeof(querySz), "UPDATE `playeraccounts` SET `playerName` = '%e' WHERE `playeraccounts`.`playerID` = '%d'", newName, playerVariables[iTarget][pInternalID]);
			mysql_tquery(handle, querySz); // No point in threading a simple response...

			mysql_format(handle, querySz, sizeof(querySz), "INSERT INTO namechanges (userid, oldname, newname, adminid) VALUES(%d, '%e', '%e', %d)", playerVariables[iTarget][pInternalID], playerName[1], newName, playerVariables[playerid][pInternalID]);
			mysql_tquery(handle, querySz);

			format(querySz, sizeof(querySz), "Administrator %s has changed your name to %s.", playerName[0], newName); // Might as well re-use the string...
			SendClientMessage(iTarget, COLOR_WHITE, querySz);

			format(querySz, sizeof(querySz), "You have changed %s (ID: %d)'s name to %s.", playerName[1], iTarget, newName);
			SendClientMessage(playerid, COLOR_WHITE, querySz);

			SetPlayerName(iTarget, newName);

			strcpy(playerVariables[iTarget][pNormalName], newName, MAX_PLAYER_NAME);
		}
	}
	else return SendClientMessage(playerid, COLOR_WHITE, AdminOnly);
	return 1;
}
