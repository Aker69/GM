					}
		        }
		    	SendToGroup(14, COLOR_TEAL, string);
		    	SendToGroup(16, COLOR_TEAL, string);
		    	SendClientMessage(playerid, COLOR_LIGHTBLUE, "* You have called for a taxi, wait at your current position.");
		    	TaxiCall = playerid;
		    	return 1;
			}
			else if(strcmp(x_nr,"medic",true) == 0)
			{
		        GetPlayerName(playerid, sendername, sizeof(sendername));
		        foreach(Player, a)
		        {
		        	if(playerVariables[a][pGroup] == 4)
		        	{
		        	   	new Float: X, Float: Y,Float: Z;
						GetPlayerPos(playerid, X,Y,Z);
		        		format(string, sizeof(string), "%s [%d] has called for a medic, a checkpoint has been set on your map.", sendername,playerid);
                        SetPlayerCheckpoint(a, X, Y, Z, 5);
					}
		        }
		    	SendToGroup(4, COLOR_TEAL, string);
		    	SendToGroup(17, COLOR_TEAL, string);
		    	SendClientMessage(playerid, COLOR_LIGHTBLUE, "* You have called for a medic, wait at your current position.");
		    	MedicCall = playerid;
		    	return 1;
			}
			else
			{
			    SendClientMessage(playerid, COLOR_GREY, "Unknown service name!");
			    return 1;
			}
		}
		return 1;
	}
	return 1;
}
