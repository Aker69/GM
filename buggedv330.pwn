CMD:contracts(playerid, params[])
{
	new group = playerVariables[playerid][pGroup];
	if(group == 12)
	{
		new dString[128], x;

		SCM(playerid, COLOR_TEAL, "---------------------------------------------------------------------------------------------------------------------------------");
		SCM(playerid, COLOR_WHITE, "Active Contract:");
		foreach(Player, i) {
			if(playerVariables[i][pHeadValue] >= 1)
			{

				GetPlayerName(i, szPlayerName, MAX_PLAYER_NAME);
				if(GotHit[i] == 1) format(dString, sizeof(dString), "Target: %s (ID:%d) - $%d - assigned\n", szPlayerName, i,playerVariables[i][pHeadValue]);
				if(GotHit[i] == 0) format(dString, sizeof(dString), "Target: %s (ID:%d) - $%d - unassigned\n", szPlayerName, i,playerVariables[i][pHeadValue]);
				SCM(playerid, COLOR_GREY, dString);

				x++;
			}
		}

		if(x == 0)
			SCM(playerid, COLOR_WHITE, "No active contracts found.");

		SCM(playerid, COLOR_TEAL, "---------------------------------------------------------------------------------------------------------------------------------");
	}

	return 1;
}
