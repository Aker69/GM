    for(new i=0; i<MAX_PLAYERS; i++)
    {
    	new saveQuery[369];
        if(IsPlayerConnected(i) && Alive[i] == 1)
        {
        	if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
			{
	            new Float:dist, Float:X, Float:Y, Float:Z;
