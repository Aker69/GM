CMD:ad(playerid, params[])
{
	if(!isnull(params))
	{
		if(playerVariables[playerid][pPhoneNumber] != -1)
		{
			if(ad[playerid] == 0)
			{
  				if(IsAtADPlace(playerid))
  				{
		  			if(playerVariables[playerid][pLevel] >= 3)
					{
						if(playerVariables[playerid][pMoney] >= 1000)
						{
							GetPlayerName(playerid, szPlayerName, MAX_PLAYER_NAME);

							playerVariables[playerid][pMoney] -= businessVariables[10][bEnterPrice];
							businessVariables[10][bVault] += businessVariables[10][bEnterPrice];
							ad[playerid] = 30;

							new addd = -1, playeradd = 0;
							for(new s = 0; s < sizeof(AdvMSG) && addd == -1; s++)
							{
							    if(strlen(AdvMSG[s]) < 10) addd = s;
							    if(strcmp(AdvPlayer[s],szPlayerName,true) == 0 && strlen(AdvPlayer[s]) == strlen(szPlayerName)) playeradd = 1;
							}
							if(addd == -1) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}Advertisement list is full.");
							if(playeradd == 1) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}You already placed an advertisement.");
							format(AdvMSG[addd], 256, "{33CC00}Ad by %s (phone: {FFFFFF}%d{33CC00}) : %s", szPlayerName,playerVariables[playerid][pPhoneNumber],params);
							format(AdvPlayer[addd], 24,"%s",szPlayerName);
							SCM(playerid,COLOR_WHITE,"You ad will be showen in a few seconds.");
							format(szMessage, 256, "New ad preview: {33CC00}Ad by %s (phone: {FFFFFF}%d{33CC00}) : %s",szPlayerName,playerVariables[playerid][pPhoneNumber],params);
							submitToHelpersAndAdmins(szMessage, COLOR_FRED);


						}
						else SendClientMessage(playerid, COLOR_GREY, "You don't have enough money for this.");
					}
					else SendClientMessage(playerid, COLOR_GREY, "You don't have enough level (3).");
				}
			}
			else return SCM(playerid,COLOR_WHITE,"You must wait 30 seconds.");
		}
		else
		{
			SendClientMessage(playerid, COLOR_GREY, "You don't have a phone, so you're unable to submit an advertisement.");
		}
	}
	else
	{
		SendClientMessage(playerid, COLOR_GREY, SYNTAX_MESSAGE"/ad [advertisement]");
	}
	return 1;
}
CMD:confiscate(playerid, params[])
{
	if(groupVariables[playerVariables[playerid][pGroup]][gGroupType] == 1) {
		new
			targetID,
			string[128],
			item[12],
			playerNames[2][MAX_PLAYER_NAME];

