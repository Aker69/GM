	            {
	                GetVehiclePos(GetPlayerVehicleID(i),X,Y,Z);
	            }
	            else
	            {
	                GetPlayerPos(i,X,Y,Z);
	            }
	            dist = floatsqroot(floatpower(floatabs(floatsub(PosX[i],X)),2)+floatpower(floatabs(floatsub(PosY[i],Y)),2)+floatpower(floatabs(floatsub(PosZ[i],Z)),2));
	            if(CheckDelay[i] >= 1)
	            {
	                CheckDelay[i]--;
	            }
