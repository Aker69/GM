		        	if(playerVariables[a][pGroup] == 14)
		        	{
		        	    new Float: X, Float: Y,Float: Z;
						GetPlayerPos(playerid, X,Y,Z);
		        		format(string, sizeof(string), "%s [%d] has called for a taxi, a checkpoint has been set on your map.", sendername,playerid);
