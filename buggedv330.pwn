				case 6:	format(szMessage, sizeof(szMessage), "%s %s: %s", groupVariables[playerVariables[playerid][pGroup]][gGroupRankName6], szPlayerName, params);
				case 7:	format(szMessage, sizeof(szMessage), "%s %s: %s", groupVariables[playerVariables[playerid][pGroup]][gGroupRankName7], szPlayerName, params);
				default: format(szMessage, sizeof(szMessage), "%s %s: %s", groupVariables[playerVariables[playerid][pGroup]][gGroupRankName1], szPlayerName, params);
			}
